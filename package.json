    "ios": "npm run build && react-native run-ios",
    "start": "npm run build && react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build": "tsc",
    "prettier": "prettier {,*/**/}*.{js,jsx,ts,tsx}"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-svg": "^12.1.0"
  },
  "peerDependencies": {
    "lodash": ">= ^4.17.19",
    "react-native-svg": ">= ^12.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.3",
    "@types/react-native": "^0.64.2",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^25.1.0",
    "eslint": "^7.14.0",
    "jest": "^25.1.0",
    "lodash": "^4.17.21",
